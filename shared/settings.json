{
    "workbench.colorTheme": "Ayu Light Bordered",
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "FiraCode Nerd Font Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 17,
    "security.workspace.trust.untrustedFiles": "open",
    "FSharp.pipelineHints.prefix": " ",
    "FSharp.pipelineHints.enabled": false,
    "FSharp.lineLens.prefix": "",
    "FSharp.inlineValues.prefix": " ",
    "FSharp.inlayHints.typeAnnotations": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
    "explorer.confirmDelete": false,
    "git.openRepositoryInParentFolders": "never",
    "editor.minimap.enabled": false,
    "FSharp.codeLenses.signature.enabled": false,
    "FSharp.inlayHints.parameterNames": false,
    "git.enableSmartCommit": true,
    "hexeditor.columnWidth": 16,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "svelte.enable-ts-plugin": true,
    "mdb.sendTelemetry": false,
    "mdb.confirmRunAll": false,
    "workbench.startupEditor": "none",
    "files.exclude": {
        "**/obj": true
    },
    "workbench.settings.applyToAllProfiles": [
        "terminal.integrated.fontFamily"
    ],
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.closingBraceHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false
}